{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "flowVersion": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "flowVersionId": {
      "type": "string",
      "pattern": "^[A-Fa-f\\d]{24}$"
    },
    "deviceId": {
      "type": "string",
      "pattern": "^[A-Fa-f\\d]{24}$"
    },
    "start": {
      "type": "string",
      "format": "date-time"
    },
    "end": {
      "type": "string",
      "format": "date-time"
    },
    "resolution": {
      "type": "number"
    },
    "metrics": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "time": {
            "type": "string",
            "format": "date-time"
          },
          "pathsFailed": {
            "type": "number"
          },
          "pathsCompleted": {
            "type": "number"
          },
          "runCount": {
            "type": "number"
          },
          "wallTime": {
            "type": "number"
          }
        }
      }
    }
  }
}