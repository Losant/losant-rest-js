{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "start": {
      "type": "string",
      "format": "date-time"
    },
    "end": {
      "type": "string",
      "format": "date-time"
    },
    "resolution": {
      "type": "number",
      "enum": [
        86400000,
        3600000
      ]
    },
    "asBytes": {
      "type": "boolean",
      "enum": [
        true,
        false
      ]
    },
    "includeNonBillable": {
      "type": "boolean",
      "enum": [
        true,
        false
      ]
    },
    "counts": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "payloads": {
            "type": "integer"
          },
          "bytes": {
            "type": "number"
          }
        }
      }
    }
  }
}