{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "sslKey": {
      "type": "string",
      "maxLength": 32767,
      "minLength": 50
    },
    "sslCert": {
      "type": "string",
      "maxLength": 32767,
      "minLength": 50
    },
    "sslBundle": {
      "type": "string",
      "maxLength": 32767,
      "minLength": 50
    },
    "domainName": {
      "type": "string",
      "maxLength": 45,
      "minLength": 3
    },
    "version": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 255
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "domainName"
  ]
}