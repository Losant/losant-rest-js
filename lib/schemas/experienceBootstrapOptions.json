{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "replaceExisting": {
      "type": "boolean",
      "default": false
    },
    "createUsers": {
      "type": "boolean",
      "default": true
    },
    "createGroups": {
      "type": "boolean",
      "default": true
    },
    "slug": {
      "type": "string",
      "minLength": 4,
      "maxLength": 63,
      "pattern": "^[0-9a-z-]*$"
    }
  },
  "additionalProperties": false
}