{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[A-Fa-f\\d]{24}$"
    },
    "experienceSlugId": {
      "type": "string",
      "pattern": "^[A-Fa-f\\d]{24}$"
    },
    "applicationId": {
      "type": "string",
      "pattern": "^[A-Fa-f\\d]{24}$"
    },
    "creationDate": {
      "type": "string",
      "format": "date-time"
    },
    "lastUpdated": {
      "type": "string",
      "format": "date-time"
    },
    "slug": {
      "type": "string",
      "minLength": 3,
      "maxLength": 63,
      "pattern": "^[0-9a-z-]*$"
    },
    "version": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    }
  }
}