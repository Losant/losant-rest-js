          "type": "integer"
        },
        "notebookMinutesPerMonth": {
          "type": "integer"
        },
        "notebookInParallel": {
          "type": "integer"
        },
        "experienceFlowSlots": {
          "type": "integer"
        },
        "applicationFlowSlots": {
          "type": "integer"
        }
      },
      "additionalProperties": false
