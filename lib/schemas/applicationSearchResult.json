{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "pattern": "^[A-Fa-f\\d]{24}$"
      },
      "name": {
        "type": "string"
      },
      "metadata": {
        "type": "object"
      },
      "viewType": {
        "type": "string"
      },
      "version": {
        "type": "string"
      },
      "_type": {
        "type": "string",
        "enum": [
          "applicationApiToken",
          "dashboard",
          "dataTable",
          "deviceRecipe",
          "device",
          "experienceDomain",
          "event",
          "experienceEndpoint",
          "experienceGroup",
          "experienceSlug",
          "experienceUser",
          "experienceVersion",
          "experienceView",
          "file",
          "flow",
          "integration",
          "webhook"
        ]
      }
    },
    "required": [
      "id",
      "name",
      "_type"
    ]
  }
}